//                    searchTAListResult searchUpcomingEventsPage searchSubscribePage searchAboutTectTimePage searchContactUsPage searchDownloadsPage searchFaqPage searchDetailMediaPage searchdetailAuthor searchBusinessCategory searchsearchResultPage
                      //  <img class="searchIcon"
                    
                    $("#searchTAListResult, #searchUpcomingEventsPage, #searchSubscribePage, #searchAboutTectTimePage, #searchContactUsPage, #searchDownloadsPage, #searchFaqPage searchDetailMediaPage searchdetailAuthor searchBusinessCategory searchsearchResultPage");
                    
                }
                
                
